{
  "compilerOptions": {
    "target": "esNext",
    "module": "esNext",
    "moduleResolution": "node",
    "lib": ["es2015", "dom", "es2016.array.include"],
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "noImplicitThis": true,
    "noUnusedParameters": false
  },
  "exclude": ["node_modules"]
}
